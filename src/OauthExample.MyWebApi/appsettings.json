{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppSettings": {
    "ClientId": "{{ Client Id Here }}",
    "ClientSecret": "{{ Client Secret Here }}",
    "SigningKey": "{{ Signing Key Here }}",
    "Issuer": "{{ Issuer Here }}",
    "ExpiresInSeconds": 3600
  },
  "AllowedHosts": "*"
}
